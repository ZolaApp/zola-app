{
  "name": "zola-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "./scripts/dev.sh",
    "build": "./scripts/build.sh",
    "start": "./scripts/start.sh",
    "icons": "./scripts/icons.sh",
    "lint:svg": "./scripts/lint-svg.sh",
    "heroku-postbuild": "./scripts/build.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "flow check && lint-staged",
      "pre-rewrite": "flow check && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-eslint": "8.2.3",
    "babel-plugin-inline-import-graphql-ast": "2.4.1",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-transform-define": "1.3.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "empty": "0.10.1",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-standard": "11.0.0",
    "eslint-config-standard-react": "6.0.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "7.9.1",
    "eslint-plugin-standard": "3.1.0",
    "flow-bin": "0.73.0",
    "husky": "0.15.0-rc.13",
    "lint-staged": "7.1.3",
    "prettier": "1.13.4",
    "spritesh": "1.2.1",
    "svgo": "1.0.5"
  },
  "dependencies": {
    "@zeit/next-css": "0.2.0",
    "apollo-boost": "0.1.7",
    "cookie": "0.3.1",
    "date-fns": "1.29.0",
    "dotenv": "6.0.0",
    "form-serialize": "0.7.2",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "isomorphic-unfetch": "2.0.0",
    "next": "6.0.3",
    "next-compose-plugins": "2.1.1",
    "next-optimized-images": "1.4.1",
    "next-routes": "1.4.2",
    "nodemon": "1.17.5",
    "nprogress": "0.2.0",
    "paginator": "1.0.0",
    "qs": "^6.5.2",
    "react": "16.4.0",
    "react-a11y-dialog": "3.0.6",
    "react-apollo": "2.1.4",
    "react-dom": "16.4.0",
    "react-toastify": "4.0.2",
    "styled-components": "3.3.2"
  }
